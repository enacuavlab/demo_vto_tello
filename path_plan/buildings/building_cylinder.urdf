<?xml version="1.0" ?>

<robot name="building">

  <link name="base_link"> 

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0" iyz="0.0" izz="0"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- <box size=".5 .5 1.5" /> -->
        <cylinder radius=".6" length="1.5"/>
      </geometry> 
      <material name="blue">
        <color rgba="0. 0. 1. 0.7"/>
      </material> 
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- <box size=".5 .5 1.5" /> -->
        <cylinder radius=".6" length="1.5"/>
      </geometry>
    </collision>  

